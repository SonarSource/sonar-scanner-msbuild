<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.6</TargetFramework>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <SonarQubeExclude>False</SonarQubeExclude>
    <ErrorLog>$(MSBuildProjectDirectory)\sarif-output.json</ErrorLog>
  </PropertyGroup>

  <ItemGroup>
    <SonarQubeSetting Include="sonar.custom.setting1">
      <Value>value1</Value>
    </SonarQubeSetting>
    <SonarQubeSetting Include="sonar.custom.setting2">
      <Value>value2</Value>
    </SonarQubeSetting>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="ExcludedFile.cs">
      <SonarQubeExclude>true</SonarQubeExclude>
    </Compile>
    <Compile Update="IncludedFile.cs">
      <SonarQubeExclude>false</SonarQubeExclude>
    </Compile>
  </ItemGroup>

</Project>
